pipeline{
    agent{ docker {image 'node:14'
            args '-u root:root'}}

    stages{
        stage ('Dep install'){
            // agent{ docker { image 'node:14'
            //     args '-u root:root'}}
            steps{
                sh 'npm install'
            }
        }
        stage ('Package npm'){
            steps{
                sh 'npm pack'
            }
        }
    }
}
